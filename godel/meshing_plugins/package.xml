<?xml version="1.0"?>
<package format="2">
  <name>meshing_plugins</name>
  <version>0.1.0</version>
  <description>
    Plugins which convert point clouds to meshes
  </description>

  <maintainer email="awood@swri.org">Aaron Wood</maintainer>
  <license>Apache 2.0</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>godel_msgs</depend>
  <depend>meshing_plugins_base</depend>
  <depend>path_planning_plugins_base</depend>
  <depend>pcl_ros</depend>
  <depend>pluginlib</depend>
  <depend>roscpp</depend>

  <export>
    <meshing_plugins_base plugin="${prefix}/plugin.xml"/>
  </export>

</package>
